  <object class="GtkAction" id="continue_action">
    <property name="label">Continue</property>
    <property name="tooltip">continue/run script</property>
    <signal name="activate" handler="on_continue_button_clicked"/>
  </object>
  <object class="GtkImage" id="ContinueImage">
    <property name="visible">True</property>
    <property name="pixbuf">run.png</property>
  </object>
  <object class="GtkImage" id="StepIntoImage">
    <property name="visible">True</property>
    <property name="pixbuf">step_in.png</property>
  </object>
  <object class="GtkAction" id="step_into_action">
    <property name="label">Step Into</property>
    <signal name="activate" handler="on_step_in_button_clicked"/>
  </object>
  <object class="GtkAction" id="step_over_action">
    <property name="label">Step Over</property>
    <signal name="activate" handler="on_step_over_button_clicked"/>
  </object>
  <object class="GtkImage" id="StepOverImage">
    <property name="visible">True</property>
    <property name="pixbuf">step_over.png</property>
  </object>
  <object class="GtkImage" id="StepOutImage">
    <property name="visible">True</property>
    <property name="pixbuf">step_out.png</property>
  </object>
  <object class="GtkImage" id="DetachImage">
    <property name="visible">True</property>
    <property name="pixbuf">detach.png</property>
  </object>
  <object class="GtkImage" id="StopImage">
    <property name="visible">True</property>
    <property name="pixbuf">dialog-error.png</property>
  </object>
  <object class="GtkAction" id="step_out_action">
    <property name="label">Step Out</property>
    <signal name="activate" handler="on_step_out_button_clicked"/>
  </object>
  <object class="GtkAction" id="detach_action">
    <property name="label">Detach</property>
    <property name="tooltip">stops the debugging session, but lets the script running</property>
    <signal name="activate" handler="on_detach_button_clicked"/>
  </object>
  <object class="GtkAction" id="stop_action">
    <property name="label">Stop</property>
    <property name="tooltip">stops the debugging session and kills the script</property>
    <signal name="activate" handler="on_kill_button_clicked"/>
  </object>
  <object class="GtkImage" id="image1">
    <property name="visible">True</property>
    <property name="stock">gtk-missing-image</property>
  </object>
  <object class="GtkImage" id="image2">
    <property name="visible">True</property>
    <property name="stock">gtk-missing-image</property>
  </object>
  <object class="GtkImage" id="image3">
    <property name="visible">True</property>
    <property name="stock">gtk-missing-image</property>
  </object>
  <object class="GtkImage" id="image4">
    <property name="visible">True</property>
    <property name="stock">gtk-missing-image</property>
  </object>
  <object class="GtkImage" id="image5">
    <property name="visible">True</property>
    <property name="stock">gtk-missing-image</property>
